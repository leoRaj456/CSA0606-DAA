def count_exists(nums1, nums2):
    set_nums2 = set(nums2)
    set_nums1 = set(nums1)
    
    answer1 = sum(1 for i in range(len(nums1)) if nums1[i] in set_nums2)
    answer2 = sum(1 for i in range(len(nums2)) if nums2[i] in set_nums1)
    
    return [answer1, answer2]
nums1 = [2,3,2]
nums2 = [1,2]
print(count_exists(nums1, nums2)) 

nums1 = [4,3,2,3,1]
nums2 = [2,2,5,2,3,6]
print(count_exists(nums1, nums2))  
